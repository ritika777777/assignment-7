#include<stdio.h>

int main()
{

int n,pre=0 ,cur=1 ,next=0,i;
printf("enter the number");
scanf("%d",&n);

for(i=0;1;i++)
    {
    next=pre+cur;
    pre=cur;
    cur=next;

    if(next==n)
    {
        printf("number found");
        break;
    }
    if(next>n)
    {
        printf("not found");
        break;

  }
    }
}

