#include<stdio.h>
#include<math.h>
int main()
{
    int n,a,b,c,sum,num;
    printf("enter a number");
    scanf("%d",&n);
    a=n/100;
    b=(n%100)/10;
    c=n%10;
    sum=n;
    num=pow(a,3)+pow(b,3)+pow(c,3);

    if(sum==num){//pow(a,3)+pow(b,3)+pow(c,3)){
    printf("Armstrong number");
    }
    else{
        printf("not Armstrong number");
    }
    return 0;
}
