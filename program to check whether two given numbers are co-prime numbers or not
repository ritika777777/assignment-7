#include<stdio.h>

int main()
{

int hcf=1,a,b,i;
printf("enter two numbers");
scanf("%d%d",&a,&b);

int min = a < b ?a  :b;
for(i=1;i<=min;i++){
    if(a%i==0 && b%i==0)
     hcf=i;
}
     if(hcf==1){
     printf("co-prime number");
     }
     else{
        printf("not co-prime number");
     }
return 0;

}
